
        A[3]=k3y;
        A[2]=k2y-(tk1 * power(a,2) + power(b,2)) * power(k1z,2) / power(c,2);
        A[1]=k1y- 2/c*((tk1 * power(a,2) + power(b,2)) * k1z * (k0z-z0) / c + (tk1 * a * x0 + b * y0) * k1z);
        A[0]=k0y - (tk1 * power(x0,2) + power(y0,2) + (tk1 * power(a,2) + power(b,2)) * power((k0z - z0), 2) / power(c, 2) + (tk1 * a * x0 + b * y0) * (k0z - z0) / c;
